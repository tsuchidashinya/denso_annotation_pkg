<?xml version="1.0"?>
<launch>
    <node pkg="mesh_cloud_package" type="mesh_cloud" name="mesh_cloud">
        <rosparam file="$(find mesh_cloud_package)/config/mesh_cloud.yaml" command="load" />
        <rosparam file="$(find annotation_common)/config/common_parameter.yaml" command="load" />
    </node>
    <node pkg="sensor_package" type="SensorServer" name="sensor_server">
        <rosparam file="$(find sensor_package)/config/sensor_server.yaml" command="load"/>
        <rosparam file="$(find annotation_common)/config/common_parameter.yaml" command="load" />
    </node>
    <node pkg="visualize_package" type="visualize_server" name="visualize_server">
        <rosparam file="$(find visualize_package)/config/visualize_server.yaml" command="load" />
        <rosparam file="$(find annotation_common)/config/common_parameter.yaml" command="load" />
    </node>
    <node pkg="labeling_package" type="record_server.py" name="record_server" output="screen">
        <rosparam file="$(find labeling_package)/config/record_server.yaml" command="load" />
    </node>

</launch>